pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                bat 'mvn -version'
            }
        }
        stage('Install') {
            steps {
                bat 'mvn clean install'
            }
        }
        stage('Test') {
            parallel{
               stage('Test'){
                  steps {
                    bat 'mvn test'
                   }
               }
            stage('Deploy') {
              steps {
               bat ' deploy'
               }
            }
          }
           post {
             always {
               cleanWs()
             }
          }
        }
    }
}
