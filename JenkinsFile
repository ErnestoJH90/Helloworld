pipeline {
    agent any

    tools {
        bat Maven '3.6.3'
    }

    stages {
        stage('Build') {
            steps {
                bat 'mvn -version'
            }
        }
        stage('Install') {
            steps {
                bat 'mvn clean install'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn test'
            }
        }
    }
        post {
         always {
            cleanWs()
    }
  }
}
