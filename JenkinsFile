pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat 'mvn -version'
            }
        }
        stage('Install') {
            steps {
                bat 'mvn clean install'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
         
    }
        post {
         always {
            cleanWs()
    }
  }
}
